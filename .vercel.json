{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/api/og/(.*)",
      "dest": "/api/og"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "DumpSack",
    "NEXT_PUBLIC_APP_DESCRIPTION": "Premium non-custodial wallet for Gorbagana and Solana",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0"
  },
  "github": {
    "silent": false
  },
  "rewrites": [
    {
      "source": "/api/og/:path*",
      "destination": "/api/og"
    }
  ]
}